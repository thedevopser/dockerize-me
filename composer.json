{
    "name": "thedevopser/dockerize-me",
    "description": "A simple Symfony bundle to dockerize easly and quickly a Symfony application",
    "type": "symfony-bundle",
    "require": {
        "symfony/console": "^7.3",
        "symfony/http-kernel": "^7.3",
        "symfony/dependency-injection": "^7.3",
        "symfony/framework-bundle": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.4",
        "symfony/phpunit-bridge": "^7.3"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Thedevopser\\DockerizeMe\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Thedevopser\\DockerizeMe\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    }
}
